{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true,
      "es6": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "no-use-before-define": 0,
      "react/prop-types": [
          "error",
          {
              "ignore": [
                  "navigation"
              ]
          }
      ],
      "import/prefer-default-export": 0
  }
}
